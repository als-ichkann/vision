<?xml version="1.0"?>
<robot name="auboi5_robot">

  <!-- 底座 -->
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder radius="0.08" length="0.165"/>
      </geometry>
      <material name="base_material">
        <color rgba="0.3 0.3 0.3 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.08" length="0.165"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="15.0"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1"/>
    </inertial>
  </link>

  <!-- 关节1 - 基座旋转 -->
  <joint name="joint_1" type="revolute">
    <parent link="base_link"/>
    <child link="link_1"/>
    <origin xyz="0 0 0.158" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14159" upper="3.14159" effort="100" velocity="2.0"/>
  </joint>

  <link name="link_1">
    <visual>
      <geometry>
        <cylinder radius="0.06" length="0.158"/>
      </geometry>
      <material name="link1_material">
        <color rgba="0.8 0.2 0.2 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.06" length="0.158"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="8.5"/>
      <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.05" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <!-- 关节2 - 肩部关节 -->
  <joint name="joint_2" type="revolute">
    <parent link="link_1"/>
    <child link="link_2"/>
    <origin xyz="0 0 0.079" rpy="0 1.5708 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-2.059" upper="2.059" effort="100" velocity="2.0"/>
  </joint>

  <link name="link_2">
    <visual>
      <geometry>
        <box size="0.1 0.16 0.425"/>
      </geometry>
      <material name="link2_material">
        <color rgba="0.2 0.8 0.2 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.1 0.16 0.425"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="11.2"/>
      <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.02"/>
    </inertial>
  </link>

  <!-- 关节3 - 肘部关节 -->
  <joint name="joint_3" type="revolute">
    <parent link="link_2"/>
    <child link="link_3"/>
    <origin xyz="0 0 0.2125" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14159" upper="3.14159" effort="80" velocity="2.0"/>
  </joint>

  <link name="link_3">
    <visual>
      <geometry>
        <box size="0.08 0.12 0.392"/>
      </geometry>
      <material name="link3_material">
        <color rgba="0.2 0.2 0.8 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.08 0.12 0.392"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="5.8"/>
      <inertia ixx="0.06" ixy="0" ixz="0" iyy="0.06" iyz="0" izz="0.01"/>
    </inertial>
  </link>

  <!-- 关节4 - 腕部旋转关节 -->
  <joint name="joint_4" type="revolute">
    <parent link="link_3"/>
    <child link="link_4"/>
    <origin xyz="0 0 0.196" rpy="1.5708 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14159" upper="3.14159" effort="50" velocity="3.0"/>
  </joint>

  <link name="link_4">
    <visual>
      <geometry>
        <cylinder radius="0.03" length="0.109"/>
      </geometry>
      <material name="link4_material">
        <color rgba="0.8 0.8 0.2 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.03" length="0.109"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2.4"/>
      <inertia ixx="0.01" ixy="0" ixz="0" iyy="0.01" iyz="0" izz="0.005"/>
    </inertial>
  </link>

  <!-- 关节5 - 腕部俯仰关节 -->
  <joint name="joint_5" type="revolute">
    <parent link="link_4"/>
    <child link="link_5"/>
    <origin xyz="0 0 0.0545" rpy="-1.5708 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-2.059" upper="2.059" effort="50" velocity="3.0"/>
  </joint>

  <link name="link_5">
    <visual>
      <geometry>
        <cylinder radius="0.025" length="0.06"/>
      </geometry>
      <material name="link5_material">
        <color rgba="0.8 0.2 0.8 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.025" length="0.06"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.8"/>
      <inertia ixx="0.008" ixy="0" ixz="0" iyy="0.008" iyz="0" izz="0.003"/>
    </inertial>
  </link>

  <!-- 关节6 - 法兰关节 -->
  <joint name="joint_6" type="revolute">
    <parent link="link_5"/>
    <child link="link_6"/>
    <origin xyz="0 0 0.03" rpy="1.5708 0 0"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14159" upper="3.14159" effort="30" velocity="3.0"/>
  </joint>

  <link name="link_6">
    <visual>
      <geometry>
        <cylinder radius="0.02" length="0.082"/>
      </geometry>
      <material name="link6_material">
        <color rgba="0.2 0.8 0.8 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <cylinder radius="0.02" length="0.082"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.8"/>
      <inertia ixx="0.005" ixy="0" ixz="0" iyy="0.005" iyz="0" izz="0.002"/>
    </inertial>
  </link>

  <!-- 相机连接 -->
  <joint name="camera_joint" type="fixed">
    <parent link="link_6"/>
    <child link="camera_link"/>
    <origin xyz="0.04 0 0.041" rpy="0 1.5708 0"/>
  </joint>

  <link name="camera_link">
    <visual>
      <geometry>
        <box size="0.05 0.05 0.03"/>
      </geometry>
      <material name="camera_material">
        <color rgba="0.1 0.1 0.1 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.05 0.05 0.03"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.5"/>
      <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
    </inertial>
  </link>

</robot>